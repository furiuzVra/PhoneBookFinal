<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="duc.example.com.phonebookfinal">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/phonebookicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/phonebookicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="duc.example.com.phonebookfinal.GetAllUsersActivity"
           />
        <activity
            android:name="duc.example.com.phonebookfinal.UpdateDeleteActivity">
        </activity>

    </application>

</manifest>